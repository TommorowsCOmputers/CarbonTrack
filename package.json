{
  "name": "CarbonTrack",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN npx expo start",
    "start": "npx expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx expo start --web",
    "regen-native": "rm -rf android ios && npx expo prebuild",
    "bundle-android": "npx react-native bundle --platform android --dev false --entry-file index.android.bundle.temp.entry.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ --reset-cache",
    "build-android-release": "cd android && ./gradlew bundleRelease",
    "build-android": "npm run regen-native && npm run bundle-android && npm run build-android-release",
    "lint": "npx expo lint",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "expo": "50.0.20",
    "react-native-stallion": "^2.3.1",
    "expo-application": "~5.8.4",
    "expo-blur": "~12.9.2",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.3",
    "expo-haptics": "~12.8.1",
    "expo-linking": "~5.0.2",
    "expo-modules-core": "~1.11.3",
    "expo-notifications": "~0.27.8",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.13",
    "firebase": "^12.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@react-native/metro-config": "^0.83.1",
    "@types/react": "^18.2.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "eas-cli": "^16.28.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "metro-react-native-babel-transformer": "^0.77.0",
    "prettier": "3.6.2",
    "typescript": "~5.9.2"
  }
}
